{
  "name": "@examples/next-websockets-starter",
  "version": "10.9.0",
  "private": true,
  "engines": {
    "node": "^18.0.0"
  },
  "packageManager": "pnpm@7.18.2",
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-next": "cross-env NODE_ENV=production next build",
    "build:3-server": "tsc --project tsconfig.server.json",
    "build:4-migrate": "prisma migrate deploy",
    "build": "run-s build:*",
    "generate-sqlite": "prisma generate --schema=./prisma/_sqlite/schema.prisma",
    "migrate-sqlite": "prisma migrate dev --name init --schema=./prisma/_sqlite/schema.prisma",
    "prep-sqlite": "run-s generate-sqlite migrate-sqlite",
    "build-sqlite": "pnpm build:3-server && next build",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "run-p dev:*",
    "dev-sqlite": "run-s generate-sqlite migrate-sqlite && run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production NEXTAUTH_URL='https://movies.peepo.dev/api/auth' APP_URL='https://movies.peepo.dev' WS_URL='wss://movies.peepo.dev' node dist/server/prodServer.js",
    "studio": "prisma studio",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev-sqlite 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "test": "playwright test",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@iconify/react": "^4.1.0",
    "@mui/material": "^5.11.6",
    "@mui/x-date-pickers": "^5.0.16",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/font": "^13.1.6",
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.24.4",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "clsx": "^1.2.1",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "moviedb-promise": "^3.3.1",
    "next": "^13.1.6",
    "next-auth": "^4.19.0",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "superjson": "^1.12.2",
    "tsx": "^3.12.2",
    "ws": "^8.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@tanstack/react-query-devtools": "^4.24.4",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.27",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "start-server-and-test": "^1.15.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
